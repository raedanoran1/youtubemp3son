{
    "framework": "flask",
    "outputDirectory": "static",
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHON_VERSION": "3.9"
    }
}
